<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data noupdate="0">
        <record id="seq_dgwindow_code" model="ir.sequence">
            <field name="name">DG Window Code</field>
            <field name="code">inseta.dgwindow.code</field>
            <field name="prefix">DG/%(year)s/</field>
            <field name="padding">3</field>
        </record>
        <record id="seq_dgapproval_code" model="ir.sequence">
            <field name="name">DG Approval Code</field>
            <field name="code">inseta.dgapproval.code</field>
            <field name="prefix">DGAPR/%(year)s/</field>
            <field name="padding">3</field>
        </record>

        <record id="seq_dgapplication_code" model="ir.sequence">
            <field name="name">DG Application Code</field>
            <field name="code">inseta.application.code</field>
            <field name="prefix">DG/dgtype/dgperiod/</field>
            <field name="padding">4</field>
        </record>

        <record id="seq_dgapplication_reference" model="ir.sequence">
            <field name="name">DG Application Reference</field>
            <field name="code">inseta.application.reference</field>
            <field name="prefix">PREFIX/%(year)s/</field>
            <field name="padding">4</field>
        </record>

        <record id="seq_heirep_reference" model="ir.sequence">
            <field name="name">DG HEI Rep. Reference</field>
            <field name="code">inseta.heirep.reference</field>
            <field name="prefix">REP/%(year)s/</field>
            <field name="padding">4</field>
        </record>

    </data>
</odoo>