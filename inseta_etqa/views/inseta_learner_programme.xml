		<record id="view_inseta_learner_programme_form" model="ir.ui.view">
			<field name="name">Learnership programme form</field>
			<field name="model">inseta.learner.programme</field>
			<field name="arch" type="xml">
				<form string="Learnership programme">
					<header> 
					</header>
					<sheet>
						<div class="oe_title">
                            <field name="active" invisible="1"/>
                            <h1>
                                <field name="name" default_focus="1"  placeholder="Name"/>
                            </h1>
                        </div>
						<group>
						<separator string="Details" colspan="4"/>
							<group>
								<field name="learnership_saqa_id" string="Learnership SAQA ID" required="1"/>
								<field name="programme_code" string="Programme code" required="1"/>
								<field name="programme_type_id" invisible="1" required="0"/>
								<field name="nqflevel_id" required="1"/>
                                <!-- <field name="ofo_occupation_id" required="0"/> -->
								<field name="qualification_type_id" required="0"/>
								<field name="learner_programme_type_id" required="0"/>
								<field name="is_unit_base" invisible="1"/>
								<field name="quality_assurance_id" required="1"/>
								<field name="registration_start_date" readonly="0" required="1"/>
								<field name="registration_end_date" readonly="0" required="1"/>
								<field name="is_south_african_programme" invisible="1"/>
							</group>
							<group>
								<field name="qualification_id" attrs="{'required': [('is_unit_base', '=', 'Yes')]}"/>
								<!-- <field name="minimum_credits" required="0"/> -->
								<field name="fundamental_credits" readonly="1" force_save="1"/>
								<field name="core_credits" readonly="1" force_save="1"/>
								<field name="elective_credits" readonly="1" force_save="1"/>
								<field name="credits" required="1"/>
								<field name="active" readonly="0"/>
								
								<!-- <field name="programme_unit_id"/> -->
							</group> 
						</group>
						<notebook>
							<page name="learnership_unit_standard_ids" string="Learnership Unit Standard line">
								<field name="learnership_unit_standard_ids" nolabel="1" readonly="0" required="0" context="{'default_learner_programme_id': active_id}" force_save="1">
									<form string="Inseta Learnership Unit Standard">
										<!-- <div class="oe_title">
											<h1> Title <br/>
												<field name="name" placeholder="" readonly="0"/>
											</h1>
										</div> -->
										<group> 
											<group>
												
												<field name="learner_programme_id" string="Learnership" required="0" invisible="1"/>
												<!-- <field name="code" required="0"/> -->
												<field name="unit_standard_id" required="1"/>
												<field name="unit_standard_type_id" required="1" invisible="0"/> 
												<field name="credits" required="1"/>
											</group>
											<group>
												<field name="legacy_system_id" invisible="0"/>
												<field name="active"/>
											</group> 
										</group>
									</form>
									<tree string="Learnership ">
										<field name="learner_programme_id" string="Learnership" required="0" invisible="1"/>
										<!-- <field name="code" required="0"/> -->
										<field name="unit_standard_id" required="1"/>
										<field name="unit_standard_type_id" required="1" invisible="0"/> 
										<field name="credits" required="1"/>
									</tree>
								</field>
							</page>
							<page name="unitstandards" string="Unit Standards line" invisible="1">
								<field name="unit_standard_ids" nolabel="1" readonly="0" required="1">
									<form string="Inseta Unit Standard">
										<div class="oe_title">
											<h1>
												<field name="name" string="Learnership code" placeholder="Name" readonly="0"/>
											</h1>
										</div>
										<group> 
											<group>
												<field name="unit_standard_type" required="1" invisible="0"/> 
												<field name="code" string="Learnership code" required="1"/>
												<field name="nqflevel_id" required="1"/>
												
											</group>
											<group>
												<field name="core_credits" invisible="0"/>
												<field name="elective_credits" required="0"/>
												<field name="fundamental_credits" invisible="0"/>
												<field name="credits" required="1"/>
											</group> 
										</group>
									</form>
									<tree string="Unit standards">
										<field name="name" required="1" invisible="0"/> 
										<field name="unit_standard_type" required="1" invisible="0"/> 
										<field name="code" required="1"/>
										<field name="nqflevel_id" required="1"/>
										<field name="fundamental_credits" invisible="0" string="Fundamental"/>
										<field name="core_credits" invisible="0" string="Core"/>
										<field name="elective_credits" required="0" string="Elective"/>
										<field name="credits" required="1"/>
									</tree>
								</field>
							</page>
						</notebook>
						  
                    </sheet>
				</form>
			</field>
		</record>

