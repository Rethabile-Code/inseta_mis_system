<?xml version="1.0" encoding="utf-8"?>
    <odoo>

		<record id="view_inseta_main_programme_form" model="ir.ui.view">
			<field name="name">programme form</field>
			<field name="model">inseta.programme</field>
			<field name="arch" type="xml">
				<form string="Programme">
					<header> 
					</header>
					<sheet>
						<div class="oe_title">
                            <field name="active" invisible="1"/>
                            <h1>Title <br/>
                                <field name="name" default_focus="1" required="1"/>
                            </h1>
                        </div>
						<group>
						<separator string="Details" colspan="4"/>
							<group>
								<field name="programme_code" string="Programme code" required="1"/>
								<field name="programme_type_id" invisible="1" required="0"/>
								<field name="nqflevel_id" required="1"/>
                                <!-- <field name="ofo_occupation_id" required="0"/> -->
							</group>
							<group>
								<field name="qualification_id" required="1"/>
								<field name="credits" required="1"/>
								<field name="quality_assurance_id"/>
								<!-- <field name="programme_unit_id"/> -->
							</group> 
						</group>
						  
                    </sheet>
				</form>
			</field>
		</record>

		<record id="view_inseta_main_programme_tree" model="ir.ui.view">
			<field name="name">view.inseta.programme.tree</field>
			<field name="model">inseta.programme</field>
			<field name="arch" type="xml">
				<tree string="" create="0" sample="1" multi_edit="1">
                    <field name="name" string="Name" required="1"/>
                    <field name="programme_code" string="Programme code" required="1"/>
                    <field name="programme_type_id" invisible="1"/>
                    <field name="nqflevel_id" required="1"/>
                    <field name="qualification_id" required="1"/>
                    <field name="credits" required="1"/>
                    <field name="quality_assurance_id"/>
                    <field name="programme_unit_id"/>
					<!-- <field name="ofo_occupation_id" required="0"/> -->
                </tree>

			</field>
		</record>

		<record id="view_inseta_main_programme_filter" model="ir.ui.view">
			<field name="name">Learnership programme</field>
			<field name="model">inseta.programme</field>
			<field name="arch" type="xml">
				<search string="Search">
					<field name="name" />
                    <field name="programme_code" string="Programme code"/>
                    <field name="quality_assurance_id"/>
